    local val = tonumber(SpeedInput.Text)
    if val and val >= 1 and val <= 50000 then
        Config.SpeedMultiplier = math.floor(val)
        UpdateUI()
    else
        SpeedInput.Text = tostring(Config.SpeedMultiplier)
    end
end)

