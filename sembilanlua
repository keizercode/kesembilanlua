-- ULTRA EXTREME HOOK SYSTEM
local function SetupUltraHooks()
    if Config.HookActive then return end
    Config.HookActive = true
    
    -- Ultra Aggressive __namecall Hook
    local oldNamecall
    oldNamecall = hookmetamethod(game, "__namecall", function(self, ...)
        local method = getnamecallmethod()
        local args = {...}
        
        if not Config.Enabled then
            return oldNamecall(self, ...)
        end
        
        -- ULTRA: Hook FireServer
        if method == "FireServer" then
            if self.Name == "TrainSpeedHasChanged" then
                if args[1] and tonumber(args[1]) then
                    args[1] = args[1] * Config.SpeedMultiplier
                    Stats.TrainCount = Stats.TrainCount + 1
                    Stats.RequestsPerSecond = Stats.RequestsPerSecond + 1
                end
            end
            
            -- Force multiply ALL numeric args in aggressive mode
            if Config.AggressiveMode or Config.ForceMultiply then
                for i, arg in ipairs(args) do
                    if typeof(arg) == "number" and arg > 0 and arg < 1e10 then
                        args[i] = arg * Config.Multiplier
                    end
                end
            end
            
            return oldNamecall(self, unpack(args))
        end
        
        -- ULTRA: Hook InvokeServer
        if method == "InvokeServer" then
            -- Multiply inputs
            if Config.ForceMultiply then
                for i, arg in ipairs(args) do
                    if typeof(arg) == "number" and arg > 0 and arg < 1e10 then
                        args[i] = arg * Config.Multiplier
                    end
                end
            end
            
            local results = {pcall(oldNamecall, self, unpack(args))}
            local success = table.remove(results, 1)
            
            if success and Config.Enabled then
                -- Multiply ALL returns
                for i, result in ipairs(results) do
                    if typeof(result) == "number" and result > 0 then
                        local originalValue = result
                        results[i] = result * Config.Multiplier
                        Stats.TotalGains = Stats.TotalGains + (results[i] - originalValue)
                    end
                end
            end
            
            return unpack(results)
        end
        
        return oldNamecall(self, ...)
    end)
    
    print("✅ Ultra Hook System Active!")
end

-- Hook ALL BindableFunctions (Ultra Fast)
task.spawn(function()
    task.wait(0.5)
    
    local hooked = 0
    for _, descendant in pairs(ReplicatedStorage:GetDescendants()) do
        if descendant:IsA("BindableFunction") then
            pcall(function()
                local oldInvoke = descendant.Invoke
                descendant.Invoke = function(self, ...)
                    local results = {oldInvoke(self, ...)}
                    
                    if Config.Enabled then
                        for i, result in ipairs(results) do
                            if typeof(result) == "number" and result > 0 then
                                results[i] = result * Config.Multiplier
                            end
                        end
                    end
                    
                    return unpack(results)
                end
                hooked = hooked + 1
            end)
        end
    end
    print(string.format("✅ Hooked %d BindableFunctions", hooked))
end)

-- Monitor ALL RemoteEvents (Ultra Fast)
task.spawn(function()
    task.wait(0.5)
    
    local monitored = 0
    for _, descendant in pairs(ReplicatedStorage:GetDescendants()) do
        if descendant:IsA("RemoteEvent") then
            pcall(function()
                descendant.OnClientEvent:Connect(function(...)
                    if Config.Enabled then
                        local args = {...}
                        for _, arg in ipairs(args) do
                            if typeof(arg) == "number" and arg > 0 and arg < 1e12 then
                                Stats.TotalGains = Stats.TotalGains + (arg * Config.Multiplier)
                            end
                        end
                    end
                end)
                monitored = monitored + 1
            end)
        end
    end
    print(string.format("✅ Monitoring %d RemoteEvents", monitored))
end)

-- ULTRA EXTREME Multi-Thread Auto Train
local function UltraTrainThread()
    while task.wait(Config.SpamRate) do
        if Config.AutoTrain and Config.Enabled then
            Stats.ThreadsActive = Stats.ThreadsActive + 1
            
            pcall(function()
                -- Use cached remotes for SPEED
                if CachedRemotes.StationaryTrain then
                    CachedRemotes.StationaryTrain:InvokeServer()
                end
                
                if CachedRemotes.MobileTrain then
                    CachedRemotes.MobileTrain:InvokeServer()
                end
                
                if Config.HyperMode then
                    if CachedRemotes.BindingEffect then
                        CachedRemotes.BindingEffect:InvokeServer("Training_2001", "Emit_2")
                    end
                    
                    if CachedRemotes.BoostEffect then
                        CachedRemotes.BoostEffect:InvokeServer()
                    end
                end
            end)
            
            Stats.ThreadsActive = math.max(0, Stats.ThreadsActive - 1)
        end
    end
end

-- Start multiple training threads
task.spawn(function()
    while task.wait(1) do
        if Config.AutoTrain and Config.Enabled and Config.MultiThread then
            local currentThreads = Stats.ThreadsActive
            local targetThreads = Config.ThreadCount
            
            -- Spawn more threads if needed
            if currentThreads < targetThreads then
                for i = 1, targetThreads do
                    task.spawn(UltraTrainThread)
                end
            end
        end
    end
end)

-- ULTRA Speed Spam (1000+ requests/second in Hyper Mode)
task.spawn(function()
    while true do
        local waitTime = Config.HyperMode and 0.001 or 0.01
        task.wait(waitTime)
        
        if Config.Enabled and Config.HyperMode then
            pcall(function()
                if CachedRemotes.TrainSpeed then
                    CachedRemotes.TrainSpeed:FireServer(999999999 * Config.SpeedMultiplier)
                    Stats.RequestsPerSecond = Stats.RequestsPerSecond + 1
                end
            end)
        end
    end
end)

-- Performance Monitor (Update stats)
task.spawn(function()
    local lastGains = 0
    local lastRequests = 0
    
    while task.wait(1) do
        if Config.Enabled then
            Stats.SessionTime = Stats.SessionTime + 1
            Stats.GainsPerSecond = Stats.TotalGains - lastGains
            lastGains = Stats.TotalGains
            
            -- Calculate actual request rate
            local actualRequests = Stats.RequestsPerSecond - lastRequests
            lastRequests = Stats.RequestsPerSecond
            
            UpdateUI()
        else
            Stats.RequestsPerSecond = 0
        end
    end
end)

-- Smooth glow animation
task.spawn(function()
    local rotation = 0
    while task.wait(0.03) do
        rotation = (rotation + 2) % 360
        MainGlow.Rotation = rotation
        
        if Config.Enabled then
            MainStroke.Color = Color3.fromRGB(
                math.floor(127 + 128 * math.sin(rotation * 0.1)),
                math.floor(127 + 128 * math.sin(rotation * 0.1 + 2)),
                math.floor(127 + 128 * math.sin(rotation * 0.1 + 4))
            )
        end
    end
end)

