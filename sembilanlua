-- Helper Functions
local function FormatNumber(n)
    if n >= 1e18 then return string.format("%.2fQu", n / 1e18)
    elseif n >= 1e15 then return string.format("%.2fQd", n / 1e15)
    elseif n >= 1e12 then return string.format("%.2fT", n / 1e12)
    elseif n >= 1e9 then return string.format("%.2fB", n / 1e9)
    elseif n >= 1e6 then return string.format("%.2fM", n / 1e6)
    elseif n >= 1e3 then return string.format("%.2fK", n / 1e3)
    else return tostring(math.floor(n)) end
end

local function UpdateUI()
    MultTitle.Text = string.format("🔥 MULTIPLIER: x%s", FormatNumber(Config.Multiplier))
    SpeedTitle.Text = string.format("⚡ SPEED: x%s", FormatNumber(Config.SpeedMultiplier))
    ThreadTitle.Text = string.format("🔥 THREADS: %d", Config.ThreadCount)
    GainLabel.Text = string.format("💎 Total: +%s", FormatNumber(Stats.TotalGains))
    SpeedLabel.Text = string.format("⚡ Gain: %s/s", FormatNumber(Stats.GainsPerSecond))
    CountLabel.Text = string.format("🎯 Count: %s", FormatNumber(Stats.TrainCount))
    RequestLabel.Text = string.format("📡 Req: %s/s", FormatNumber(Stats.RequestsPerSecond))
    ThreadLabel.Text = string.format("🔥 Threads: %d", Stats.ThreadsActive)
end

-- Cache remotes on startup
local function CacheRemotes()
    task.spawn(function()
        local TrainSystem = ReplicatedStorage:WaitForChild("TrainSystem", 5)
        if TrainSystem then
            local Remote = TrainSystem:WaitForChild("Remote", 3)
            if Remote then
                CachedRemotes.TrainSpeed = Remote:FindFirstChild("TrainSpeedHasChanged")
            end
        end
        
        local TrainEquip = ReplicatedStorage:WaitForChild("TrainEquipment", 5)
        if TrainEquip then
            local Remote = TrainEquip:WaitForChild("Remote", 3)
            if Remote then
                CachedRemotes.StationaryTrain = Remote:FindFirstChild("ApplyStationaryTrain")
                CachedRemotes.MobileTrain = Remote:FindFirstChild("ApplyMobileTrain")
                CachedRemotes.BindingEffect = Remote:FindFirstChild("ApplyBindingTrainingEffect")
                CachedRemotes.BoostEffect = Remote:FindFirstChild("ApplyBindingTrainingBoostEffect")
            end
        end
        print("✅ Remotes Cached!")
    end)
end

-- Input Handlers
MultInput.FocusLost:Connect(function()
    local val = tonumber(MultInput.Text)
    if val and val >= 1 and val <= 99999999 then
        Config.Multiplier = math.floor(val)
        UpdateUI()
    else
        MultInput.Text = tostring(Config.Multiplier)
    end
end)

